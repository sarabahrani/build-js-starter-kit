{
  "name": "js-starter-kit",
  "version": "1.0.0",
  "description": "To build a customized starter kit for JavaScript",
  "main": "app.js",
  "scripts": {
    "prestart": "babel-node buildScript/startMessage.js",
    "start": "npm-run-all --parallel lint:watch open:src ",
    "open:src": "babel-node buildScript/srcServer.js",
    "lint": "esw webpack.config.* buildScript src --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain sara",
    "share": "npm-run-all --parallel  open:src localtunnel "
  },
  "repository": {
    "type": "git"
  },
  "author": "Sara Bahrani",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.18.13",
    "chalk": "^2.1.0",
    "css-loader": "^0.28.7",
    "eslint-watch": "^3.1.2",
    "localtunnel": "^1.8.3",
    "npm-run-all": "^4.1.1",
    "nsp": "^2.7.0",
    "path": "^0.12.7",
    "standard": "^10.0.3",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0"
  }
}

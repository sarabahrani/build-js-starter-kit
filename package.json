  "license": "ISC",
  "dependencies": {
    "browser-sync": "^2.18.13",
    "bs-html-injector": "^3.0.3",
    "chalk": "^2.1.0",
    "localtunnel": "^1.8.3",
    "npm-run-all": "^4.1.1",
    "nsp": "^2.7.0"
  }
}

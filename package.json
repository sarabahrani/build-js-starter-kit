{
  "name": "js-starter-kit",
  "version": "1.0.0",
  "description": "To build a customized starter kit for JavaScript",
  "main": "index.js",
  "scripts": {
    "prestart": "node buildScript/startMessage.js",
    "start": "npm-run-all --parallel  open:src ",
    "open:src":"node buildScript/srcServer.js",
    "security-check": "nsp check",
